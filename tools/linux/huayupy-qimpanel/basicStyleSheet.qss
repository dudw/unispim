/*QCheckBox的样式*/
QCheckBox
{
height: 30px;
min-height:30px;
font-size:14px;
outline:none;
}
QCheckBox:indicator{
width:14px;
height:14px;
}
QCheckBox::indicator:unchecked{
image:url(:/image/toolbarConfig/unchecked.png);
}
QCheckBox::indicator:unchecked:hover{
image:url(:/image/toolbarConfig/hover-unchecked.png);
}

QCheckBox::indicator:checked{
image:url(:/image/toolbarConfig/checked.png);
}
QCheckBox::indicator:checked:hover{
image:url(:/image/toolbarConfig/hover-checked.png);
}


