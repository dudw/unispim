/*普通按钮的样式*/
QWidget QPushButton[type="normal"]{
font-size:14px;
border:1px solid rgb(77,156,248);
border-radius:2px;
color:#FFFFFF;
background:#017BC9;
outline:none;
font-weight: 400;
}
QWidget QPushButton[type="normal"]:checked{
width:80px;
height:30px;
background:rgba(77,156,248,1);
border-radius:2px;
color:white;
}
QWidget QPushButton[type="normal"]:pressed{
width:80px;
height:30px;
background:#017BC9;
border-radius:2px;
color:#FFFFFF;
}

QWidget QPushButton[type="normal"]:hover{
width:80px;
height:30px;
background:#017BC9;
border-radius:2px;
color:#FFFFFF;
}

QWidget QPushButton[type="normal"]:!enabled{
color:rgb(128,128,128);
background:rgb(191,191,191);
border:2px solid rgb(191,191,191);
}

/*close按钮的样式*/
.QPushButton[type="closebtn"]{
image:url(:/image/toolbarConfig/left_btn_close.png);
border:0px solid;
width:50px;
height:50px;
background:transparent;
}
.QPushButton[type="closebtn"]:hover{
image:url(:/image/toolbarConfig/left_btn_close_hover.png);
background:#FD4D4D
}
.QPushButton[type="closebtn"]:pressed{
image:url(:/image/toolbarConfig/left_btn_close_hover.png);
border:0px solid;
}

