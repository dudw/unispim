/*普通按钮的样式*/
QWidget QPushButton[type="normal"]{
font-size:14px;
font-family:Microsoft YaHei;
border:none;
border-radius:2px;
color:#4D9CF8;
background:#E6E6E6;
padding-bottom : 2px;
outline:none;
border-image: url(:/image/normal_package.png);
background-position:top right;
background-repeat: no-repeat;
}

QWidget QPushButton[type="normal"]:hover{
width:80px;
height:30px;
background:#4D9CF8;
border-radius:1px;
color:white;
border-image: url(:/image/hover_package.png);
background-position:middle middle;
background-repeat: no-repeat;
}

QWidget QPushButton[type="normal"]:checked{
width:80px;
height:30px;
background:#4D9CF8;
border-radius:2px;
color:white;
}

QWidget QPushButton[type="normal"]:pressed{
width:80px;
height:30px;
background:#2D7BD5;
border-radius:2px;
}


QWidget QPushButton[type="normal"]:!enabled{
color:rgb(128,128,128);
background:rgb(191,191,191);
border:2px solid rgb(191,191,191);
}

/*bordernormal按钮样式*/
QWidget QPushButton[type="bordernormal"]{
width:80px;
height:27px;
min-height:27px;
min-width:80px;
font-size:14px;
font-family:Microsoft YaHei;
border:1px solid rgb(77,156,248);
border-radius:13px;
color:#4D9CF8;
background:rgb(255,255,255);
}
QWidget QPushButton[type="bordernormal"]:pressed{
width:80px;
height:30px;
background:rgba(77,156,248,1);
border-radius:13px;
}
QWidget QPushButton[type="bordernormal"]:hover{
width:80px;
height:30px;
background:rgba(77,156,248,1);
border-radius:13px;
color:white;
}

QWidget QPushButton[type="bordernormal"]:!enabled{
color:rgb(128,128,128);
background:rgb(191,191,191);
border:2px solid rgb(191,191,191);
}

/*页面中的设置按钮的样式*/
QWidget QPushButton[type="normal-set-btn"]{
width:80px;
height:30px;
font-size:14px;
font-family:Microsoft YaHei;
border-radius:2px;
color:#333333;
background:transparent;
outline:none;
}
QWidget QPushButton[type="normal-set-btn"]:pressed{
width:80px;
height:27px;
border-radius:2px;
}
QWidget QPushButton[type="normal-set-btn"]:hover{
width:80px;
height:30px;
border-radius:2px;
}



/*提示按钮的样式*/
.QPushButton[type="tip"] { 
background-color : #FFFFFF;
image : url(:/image/help.png); 
border : 0px solid;
height:16px;
width:16px;
min-height:16px;
min-width:16px;
outline:none;
margin-left: 4px;
} 
.QPushButton[type="tip"]:hover { 
image : url(:/image/help_hover.png); 
border : 0px solid;
}

/*获取验证码的样式*/
.QPushButton[type="Verification_Code"] { 
border : 1px solid #4D9CF8;
padding: 2px 4px;
border-radius:0px;
border-radius:16px;
width: 75px;
height:25px;
color:#4D9CF8;
background: white;
outline:none;
} 
.QPushButton[type="Verification_Code"]:hover{
background:#4D9CF8;
border : 1px solid #4D9CF8;
color:white;
}
.QPushButton[type="Verification_Code"]:pressed{ 
border : 2px solid #3184E5;
padding: 2px 4px;
min-width: 40px;
height:25px;
border-radius:16px;
color:white;
background:#3184E5;
} 


.QPushButton[type="Verification_Code"]:!enabled {
color:rgb(128,128,128);
background:rgb(191,191,191);
border: 2px solid rgb(191,191,191);
}

/*登录按钮的样式*/
.QPushButton[type="login"] { 
border : 2px solid rgb(77,156,248);
padding: 2px 4px;
border-radius:0px;
border-radius:15px;
width: 75px;
height:25px;
color:white;
background:rgb(77,156,248);
outline:none;
} 

.QPushButton[type="login"]:hover{
background:rgb(85,118,189);
border : 2px solid rgb(85,118,189);
}

.QPushButton[type="login"]:pressed{ 
border : 2px solid rgb(85,118,160);
padding: 2px 4px;
min-width: 40px;
height:25px;
border-radius:15px;
color:white;
background:rgb(85,118,160);
} 


.QPushButton[type="login"]:!enabled {
color:rgb(128,128,128);
background:rgb(191,191,191);
border: 2px solid rgb(191,191,191);
}

/*注册按钮的样式*/
