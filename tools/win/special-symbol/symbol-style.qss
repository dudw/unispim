border-radius:0px;
height:30px;
min-height:30px;
border-radius:16px;
max-height:50px;
}

QLineEdit QPushButton#searchBtn
{
background:transparent;
border:none;
width:20px;
height:20px;
image:url(:/image/search.png);
}

QLineEdit QPushButton#closeBtn
{
background:transparent;
border: none;
